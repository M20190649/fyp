isbn = {1476-4687},
issn = {1476-4687},
journal = {Nature},
keywords = {Cellular Phone/statistics {\&} numerical data,Disast,Disaster Planning,Geographic Information Systems,Humans,Locomotion,Models,Probability,Statistical,Travel/statistics {\&} num,Travel/statistics {\&} numerical data},
number = {7196},
pages = {779--782},
pmid = {18528393},
primaryClass = {http:},
title = {{Understanding individual human mobility patterns}},
url = {http://antonioneme.googlepages.com/understanding{\_}individual{\_}human{\_}mobil.pdf$\backslash$nhttp://arxiv.org/pdf/0806.1256;$\backslash$nhttp://antonioneme.googlepages.com/understanding{\_}individual{\_}human{\_}mobil.pdf},
